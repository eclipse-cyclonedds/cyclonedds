#include "CUnit/Runner.h"

@CUnit_Decls@

int main (int argc, char *argv[])
{
    CU_ErrorCode err;

    if ((err = cu_runner_init(argc, argv))) {
        goto err_init;
    }

@CUnit_Defs@

    err = cu_runner_run();
    cu_runner_fini();
err_init:
    return err;
}

